 <div>
   <div class="col-md-8 offset-md-1">
      <h2>会員一覧</h2>
      <table class="table table-striped">
        <thead>
          <tr>
            <th>会員ID</th>
            <th>氏名</th>
            <th>メールアドレス</th>
            <th>ステータス</th>
          </tr>
        </thead>
        <tbody>
          <% @customers.each do |customer| %>
            <tr>
              <td><%= customer.customer_id %></td>
              <td>
                <%= customer.last_name %>
                <%= customer.first_name %>
              </td>
              <td><%= customer.email %></td>
              <td><%= customer.is_active %></td>
            </tr>
          <% end %>
        </tbody>
        <%= paginate @customers %>
      </table>
   </div>
 </div>