<main>
  <p id="notice"></p>
  <div class="container">
    <div class="row">
      <div class="col-sm-12 col-md-8 col-lg-5 px-5 px-sm-0 mx-auto">
        <h2>ジャンル編集</h2>
        <from enctype="multipart/form-data">

          <%= form_with model: @genre, url: admins_genre_path,method: :patch, local:true do |f| %>


            <div class="form-group">
              <label for="genres_name">ジャンル名</label>
              <%= f.text_field :name %>
              <%= f.submit "変更を保存", class: "btn btn-secondary" %>
            </div>
          <% end %>

        </from>
      </div>
    </div>
  </div>
</main>